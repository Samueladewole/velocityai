# Production Environment Variables for AWS Amplify
VITE_APP_ENV=production
VITE_API_BASE_URL=https://api.eripapp.com
VITE_APP_BASE_URL=https://app.eripapp.com
VITE_VELOCITY_BASE_URL=https://velocity.eripapp.com

# AWS Configuration
VITE_AWS_REGION=us-east-1
VITE_AWS_COGNITO_REGION=us-east-1

# Feature Flags
VITE_ENABLE_VELOCITY=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG=false

# Velocity-specific settings
VITE_VELOCITY_API_ENDPOINT=https://api.eripapp.com/v1/velocity
VITE_VELOCITY_WEBSOCKET_URL=wss://ws.eripapp.com/velocity

# Security
VITE_CSP_REPORT_URI=https://api.eripapp.com/csp-report
VITE_SENTRY_DSN=your-sentry-dsn-here